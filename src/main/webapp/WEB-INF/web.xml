<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>lode</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>ExtractOntology</servlet-name>
    <servlet-class>com.edmondchuc.lode2.ExtractOntology</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ExtractOntology</servlet-name>
    <url-pattern>/extract</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>GetSource</servlet-name>
    <servlet-class>com.edmondchuc.lode2.GetSource</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetSource</servlet-name>
    <url-pattern>/source</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>Add</servlet-name>
  	<servlet-class>com.edmondchuc.lode2.Add</servlet-class>  
  </servlet>
  <servlet-mapping>
  	<servlet-name>Add</servlet-name>
  	<url-pattern>/add</url-pattern>
  </servlet-mapping>
</web-app>