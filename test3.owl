<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:dc11="http://purl.org/dc/elements/1.1/"
         xmlns:dc="http://purl.org/dc/terms/"
         xmlns:ns0="http://purl.org/vocab/vann/"
         xmlns:ns1="http://purl.org/vocommons/voaf#"
         xmlns:void="http://rdfs.org/ns/void#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#">

  <rdf:Description rdf:about="http://promsns.org/def/proms">
    <rdf:type rdf:resource="http://purl.org/vocommons/voaf#Vocabulary"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
    <dc11:creator>Nicholas J. Car</dc11:creator>
    <dc11:description xml:lang="en">All classes in this ontology are subclasses (specialisations) of PROV ontology classes.</dc11:description>
    <dc11:publisher>CSIRO</dc11:publisher>
    <dc11:rights xml:lang="en">Copyright 2016 CSIRO</dc11:rights>
    <dc11:title xml:lang="en">PROvenance Management System Ontology</dc11:title>
    <dc:created rdf:datatype="http://www.w3.org/2001/XMLSchema#gYear">2014</dc:created>
    <dc:hasPart>BasicReport</dc:hasPart>
    <dc:hasPart>EntityComponent</dc:hasPart>
    <dc:hasPart>ExternalReport</dc:hasPart>
    <dc:hasPart>InternalReport</dc:hasPart>
    <dc:hasPart>Report</dc:hasPart>
    <dc:hasPart>ReportingSystem</dc:hasPart>
    <dc:hasPart>endingActivity</dc:hasPart>
    <dc:hasPart>nativeId</dc:hasPart>
    <dc:hasPart>query</dc:hasPart>
    <dc:hasPart>reportingSystem</dc:hasPart>
    <dc:hasPart>startingActivity</dc:hasPart>
    <dc:license rdf:resource="https://wiki.csiro.au/pages/viewpage.action?pageId=663847169"/>
    <dc:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2016-10-01</dc:modified>
    <ns0:preferredNamespacePrefix>proms</ns0:preferredNamespacePrefix>
    <ns0:preferredNamespaceUri>http://promsns.org/def/proms#</ns0:preferredNamespaceUri>
    <ns1:extends rdf:resource="http://www.w3.org/ns/prov#"/>
    <void:uriSpace rdf:parseType="Literal">http://promsns.org/def/proms#</void:uriSpace>
    <void:vocabulary>http://promsns.org/def/proms</void:vocabulary>
    <rdfs:comment xml:lang="en">This ontology is a specialisation of the PROV ontology. It adds several subclasses to Bundle to cater for types of reports containing provenance from reporting systems (a subclass of Agent). The attributes of these reports allow provenance from heterogeneous systems to be managed in a single provenance store.</rdfs:comment>
    <rdfs:label xml:lang="en">Provenance Management System Ontology</rdfs:label>
    <owl:imports rdf:resource="http://purl.org/dc/elements/1.1/"/>
    <owl:imports rdf:resource="http://purl.org/dc/terms/"/>
    <owl:imports rdf:resource="http://purl.org/vocab/vann/"/>
    <owl:imports rdf:resource="http://purl.org/vocommons/voaf#"/>
    <owl:imports rdf:resource="http://rdfs.org/ns/void"/>
    <owl:imports rdf:resource="http://www.w3.org/2000/01/rdf-schema"/>
    <owl:imports rdf:resource="http://www.w3.org/ns/prov-o"/>
    <owl:imports rdf:resource="http://xmlns.com/foaf/0.1/"/>
    <owl:versionInfo>1.2</owl:versionInfo>
  </rdf:Description>

  <owl:Class rdf:about="http://promsns.org/def/proms#BasicReport">
    <rdfs:comment xml:lang="en">A Report for which no other information is known that its ReportingSystem's nativeId and a generatedAtTime. It is little more than a log entry with few details.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://promsns.org/def/proms"/>
    <rdfs:label xml:lang="en">Basic Report</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://promsns.org/def/proms#Report"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <owl:onProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#comment"/>
      </owl:Restriction>
    </rdfs:subClassOf>

  </owl:Class>

  <owl:Class rdf:about="http://promsns.org/def/proms#ExternalReport">
    <rdfs:comment xml:lang="en">A Report for which the identities of the data items (Entities) that the ReportingSystem that generated it are known but no internal process steps. An ExternalReport is a 'back box' with regards to its ReportingSystem's internal processes.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://promsns.org/def/proms"/>
    <rdfs:label xml:lang="en">External Report</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://promsns.org/def/proms#Report"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
        <owl:onProperty rdf:resource="http://promsns.org/def/proms#endingActivity"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
        <owl:onProperty rdf:resource="http://promsns.org/def/proms#startingActivity"/>
      </owl:Restriction>
    </rdfs:subClassOf>

  </owl:Class>

  <owl:Class rdf:about="http://promsns.org/def/proms#InternalReport">
    <rdfs:comment xml:lang="en">A Report for which details of the ReportingSystem that generated its internal details are known. The internal details are a series of one or more prov:Activity class instances with appropriate handover prov:Entity class instances with at least known input Entities to the first Activity and Entity outputs from the last Activity.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://promsns.org/def/proms"/>
    <rdfs:label xml:lang="en">Internal Report</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://promsns.org/def/proms#Report"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
        <owl:onProperty rdf:resource="http://promsns.org/def/proms#endingActivity"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
        <owl:onProperty rdf:resource="http://promsns.org/def/proms#startingActivity"/>
      </owl:Restriction>
    </rdfs:subClassOf>

  </owl:Class>

  <owl:Class rdf:about="http://promsns.org/def/proms#Report">
    <rdfs:comment xml:lang="en">Reports are the outputs of a single ReportingSystem operation. They are a single graph describing some provenance for the output od the ReportingSystem's operation's outputs.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://promsns.org/def/proms"/>
    <rdfs:label xml:lang="en">Report</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Bundle"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://promsns.org/def/proms#nativeId">
            <rdfs:comment xml:lang="en">An ID that the ReportingSystem that generated this Report allocates to it. This is used to ensure there is an ID present in PROMS Reports that ReportingSystem owners cna use to find Reports</rdfs:comment>
            <rdfs:domain rdf:resource="http://promsns.org/def/proms#Report"/>
            <rdfs:isDefinedBy rdf:resource="http://promsns.org/def/proms"/>
            <rdfs:label xml:lang="en">native ID</rdfs:label>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
          </owl:ObjectProperty>
        </owl:onProperty>

      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://promsns.org/def/proms#wasReportedBy">
            <rdfs:comment xml:lang="en">Each Report must be associated with a ReportingSystem instance</rdfs:comment>
            <rdfs:domain rdf:resource="http://promsns.org/def/proms#Report"/>
            <rdfs:isDefinedBy rdf:resource="http://promsns.org/def/proms"/>
            <rdfs:label xml:lang="en">From a Reporting System</rdfs:label>
            <rdfs:range>
              <owl:Class rdf:about="http://promsns.org/def/proms#ReportingSystem">
                <rdfs:comment xml:lang="en">A ReportingSystem is a SoftwareAgent, computer or other system that is able to produce Reports, therefore it is a system that is able to trace some level of provenance for processes that it undertakes. It must be acting on behalf of another Agent (usually a prov:Person) when producing Reports.</rdfs:comment>
                <rdfs:isDefinedBy rdf:resource="http://promsns.org/def/proms"/>
                <rdfs:label xml:lang="en">Reporting System</rdfs:label>
                <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Agent"/>
                <rdfs:subClassOf>
                  <owl:Restriction>
                    <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
                    <owl:onProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#label"/>
                  </owl:Restriction>
                </rdfs:subClassOf>

                <rdfs:subClassOf>
                  <owl:Restriction>
                    <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
                    <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#actedOnBehalfOf"/>
                  </owl:Restriction>
                </rdfs:subClassOf>

              </owl:Class>
            </rdfs:range>

            <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#wasAttributedTo"/>
          </owl:ObjectProperty>
        </owl:onProperty>

      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
        <owl:onProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#label"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
        <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#generatedAtTime"/>
      </owl:Restriction>
    </rdfs:subClassOf>

  </owl:Class>

  <owl:ObjectProperty rdf:about="http://promsns.org/def/proms#endingActivity">
    <rdfs:comment xml:lang="en">The final Activity within a Report, based on endedAtTime.</rdfs:comment>
    <rdfs:domain rdf:resource="http://promsns.org/def/proms#Report"/>
    <rdfs:isDefinedBy rdf:resource="http://promsns.org/def/proms"/>
    <rdfs:label xml:lang="en">ending Activity</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Activity"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://promsns.org/def/proms#startingActivity">
    <rdfs:comment xml:lang="en">The initial Activity within a Report, based on startedAtTime.</rdfs:comment>
    <rdfs:domain rdf:resource="http://promsns.org/def/proms#Report"/>
    <rdfs:isDefinedBy rdf:resource="http://promsns.org/def/proms"/>
    <rdfs:label>starting Activity</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Activity"/>
  </owl:ObjectProperty>

</rdf:RDF>